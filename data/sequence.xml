<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_hospital_appointment" model="ir.sequence">
                <field name="name">Appointment Reference</field>
                <field name="code">hospital.appointment</field>
                <field name="prefix">HMS</field>
                <field name="padding">6</field>
                <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_hospital_patient" model="ir.sequence">
                <field name="name">Patient Reference</field>
                <field name="code">hospital.patient</field>
                <field name="prefix">HMS</field>
                <field name="padding">6</field>
                <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>